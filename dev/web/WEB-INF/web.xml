		<env-entry-value>service_names</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>host_path_mz_ec</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>/usr/sbin/host</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>mzplatformport</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>9000</env-entry-value>
	</env-entry>
	<env-entry>
	    <env-entry-name>mzecport</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>9090</env-entry-value>
	</env-entry>
	<env-entry>
