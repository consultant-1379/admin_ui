	<!--
		Authentication mechanism for ENIQ Events administration.
	-->
	<!-- Secure the application    -->
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>Secure All Resources</web-resource-name>
			<url-pattern>/*</url-pattern>
			<http-method>GET</http-method>
			<http-method>POST</http-method>
		</web-resource-collection>
		<web-resource-collection>
		 <web-resource-name>Automatic SSL Forwarding</web-resource-name>
            <url-pattern>/*</url-pattern>
        </web-resource-collection>
        <user-data-constraint>
            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
        </user-data-constraint>
		<auth-constraint>
		    <!-- Events use role Admin Users and Stats use role admin -->
			<role-name>Admin Users</role-name>
			<role-name>eniq</role-name>
		</auth-constraint>
	</security-constraint>
	
	<!--
		EQEV-129619 : Directory Traversal - Configuration file exposed to browser
	-->	
	<security-constraint>
        <web-resource-collection>
            <web-resource-name>Secure Conf</web-resource-name>
            <url-pattern>/conf/*</url-pattern>
        </web-resource-collection>
        <auth-constraint/>
    </security-constraint>
	
	<security-constraint>
        <web-resource-collection>
            <web-resource-name>Secure HTML</web-resource-name>
            <url-pattern>/colorlegend.html</url-pattern>
        </web-resource-collection>
        <auth-constraint/>
    </security-constraint>

	<!--
		Unsecure some of the resource required by the login screen images etc
	-->
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>Unsecure the login directory</web-resource-name>
			<description />
			<url-pattern>/eniq_blue_banner.jpg</url-pattern>
			<url-pattern>/eniq_logo.gif</url-pattern>
			<url-pattern>/img/*</url-pattern>
			<url-pattern>/css/*</url-pattern>
			<url-pattern>/login.jsp</url-pattern>
			<url-pattern>/loginError.jsp</url-pattern>
			<url-pattern>/index.jsp</url-pattern>
            <url-pattern>/eric.ico</url-pattern>
            <url-pattern>/javascript/PreventXSS.js</url-pattern>      
		</web-resource-collection>
		<user-data-constraint>
        <transport-guarantee>CONFIDENTIAL</transport-guarantee>
    </user-data-constraint>
	</security-constraint>

	<login-config>
		<auth-method>FORM</auth-method>
		<realm-name>Ericsson Network IQ Management Interface Authentication</realm-name>
		<form-login-config>
			<form-login-page>/login.jsp</form-login-page>
			<form-error-page>/loginError.jsp</form-error-page>
		</form-login-config>
	</login-config>
	<security-role>
		<role-name>Admin Users</role-name>
	</security-role>
	<security-role>
		<role-name>eniq</role-name>
	</security-role>


